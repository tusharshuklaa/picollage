{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2015
    },
    "globals": {
    },
    "rules": {
        "indent": [
            "warn",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": "warn",
        "max-lines-per-function": [
          "error",
          {
            "max": 30,
            "skipComments": true,
            "skipBlankLines": true,
            "IIFEs": false
          }
        ],
        "no-alert":"error",
        "no-console":"off"
    }
}
